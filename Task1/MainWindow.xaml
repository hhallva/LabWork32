<Window x:Class="Task1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Task1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <ListBox x:Name="subjectsListBox">
            <CheckBox Content="Литература"/>
            <CheckBox Content="Математика"/>
            <CheckBox Content="Физика"/>
            <CheckBox Content="История"/>
            <CheckBox Content="География"/>
        </ListBox>
        <Button  x:Name="ViewCheckedButton" Content="Вывод" Click="ViewCheckedButton_Click"/>
    </StackPanel>
</Window>
